        "exclude": [
          "obj/**",
          "docs/generated/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "docs/generated/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "docs/generated/**"
        ]
      }
    ],
    "dest": "docs/generated",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
