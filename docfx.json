{
  "metadata": [
    {
      "src": [
        {
          "files": [ "MetadataExtractor/MetadataExtractor.net45.csproj" ]
        }
      ],
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
